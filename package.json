{
  "name": "rollup-react-redux",
  "version": "0.0.1",
  "description": "React + Meteor + Redux + Rollup",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "build:client": "rollup -c rollup/rollup.client.js",
    "build:server": "rollup -c rollup/rollup.server.js",
    "build:watch": "watch 'npm run build' src/",
    "build": "npm run build:server && npm run build:client",
    "start": "cd _meteor && meteor --port 3001"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tungv/rollup-react-redux.git"
  },
  "keywords": [
    "react",
    "meteor",
    "redux",
    "rollup"
  ],
  "author": "Tung Vu <me@tungv.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tungv/rollup-react-redux/issues"
  },
  "homepage": "https://github.com/tungv/rollup-react-redux#readme",
  "devDependencies": {
    "babel-preset-es2015-rollup": "^1.1.1",
    "rollup-plugin-json": "^2.0.0"
  },
  "dependencies": {
    "rollup": "^0.26.0"
  }
}
